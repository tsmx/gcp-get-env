{
  "name": "gcp-get-env",
  "version": "1.0.0",
  "description": "Simple retrieving of Google Cloud Platform (GCP) provided default process.env variables in cloud functions (gen1 and gen2) and app engine for Node.js 18, 20 and 22.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "true",
    "deploy-node18-func": "gcloud functions deploy node18-get-env --runtime=nodejs18 --max-instances=1 --region=europe-west3 --source=./functions/ --entry-point=getEnv --trigger-http --allow-unauthenticated --docker-registry=artifact-registry",
    "deploy-node18-func-gen2": "gcloud functions deploy node18-gen2-get-env --runtime=nodejs18 --max-instances=1 --entry-point=getEnv2 --region=europe-west3 --trigger-http --allow-unauthenticated",
    "deploy-node20-func": "gcloud functions deploy node20-get-env --runtime=nodejs20 --max-instances=1 --region=europe-west3 --source=./functions/ --entry-point=getEnv --trigger-http --allow-unauthenticated --docker-registry=artifact-registry",
    "deploy-node20-func-gen2": "gcloud functions deploy node20-gen2-get-env --runtime=nodejs20 --max-instances=1 --region=europe-west3 --entry-point=getEnv2 --trigger-http --allow-unauthenticated",
    "deploy-node22-func-gen2": "gcloud functions deploy node22-gen2-get-env --runtime=nodejs22 --max-instances=1 --region=europe-west3 --entry-point=getEnv2 --trigger-http --allow-unauthenticated",
    "deploy-node18-gae": "gcloud app deploy -q ./app-node18.yaml",
    "deploy-node20-gae": "gcloud app deploy -q ./app-node20.yaml",
    "deploy-node22-gae": "gcloud app deploy -q ./app-node22.yaml"
  },
  "author": "tsmx <dev@tsmx.net>",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.4.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "eslint": "^8.47.0"
  },
  "keywords": [
    "GCP",
    "cloud functions",
    "app engine",
    "env"
  ]
}
